{
    "name": "@nzyme/crypto-utils",
    "version": "0.5.0",
    "sideEffects": false,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/kedrzu/nzyme.git"
    },
    "author": "Michał Kędrzyński <m.kedrzynski@gmail.com>",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/cjs/index.node.js",
    "module": "./dist/esm/index.node.js",
    "types": "./dist/esm/index.node.d.ts",
    "exports": {
        "node": {
            "import": "./dist/esm/index.node.js",
            "require": "./dist/cjs/index.node.js",
            "types": "./dist/esm/index.node.d.ts"
        },
        "default": {
            "import": "./dist/esm/index.browser.js",
            "require": "./dist/cjs/index.browser.js",
            "types": "./dist/esm/index.browser.d.ts"
        }
    },
    "scripts": {
        "eslint": "eslint . --fix --cache",
        "tsc:esm": "tsc --build ./tsconfig.esm.json",
        "tsc:cjs": "tsc --build ./tsconfig.cjs.json",
        "smoke:mjs": "node tests/smoke.test.mjs",
        "smoke:cjs": "node tests/smoke.test.cjs"
    },
    "dependencies": {
        "@nzyme/types": "^0.5.0",
        "@nzyme/utils": "^0.5.0",
        "spark-md5": "^3.0.2",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@nzyme/eslint": "^0.5.0",
        "@nzyme/tsconfig": "^0.5.0",
        "@nzyme/vitest": "^0.5.0",
        "@types/node": "^22.10.7",
        "@types/spark-md5": "^3.0.5"
    },
    "depcheck": {
        "ignoreFiles": [
            "tests"
        ]
    }
}
